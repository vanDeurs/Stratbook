{
  "name": "heroku-cra-node",
  "version": "1.0.0",
  "description": "How to use create-react-app with a custom Node API on Heroku",
  "engines": {
    "node": "8.9.x"
  },
  "scripts": {
    "start": "nodemon server",
    "heroku-postbuild": "cd react-ui/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.1.1",
    "express": "^4.14.1",
    "fs": "0.0.1-security",
    "nodemon": "^1.17.3",
    "prop-types": "^15.6.1",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.3.2",
    "react-materialize": "^2.2.0",
    "react-modal": "^3.4.4",
    "react-router-dom": "^4.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vanDeurs/stratbook"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {}
}
